#![enable(implicit_some)]
#![enable(unwrap_newtypes)]
#![enable(unwrap_variant_newtypes)]
(
    default_album_art_path: None,
    show_song_table_header: true,
    draw_borders: true,
    format_tag_separator: " | ",
    browser_column_widths: [20, 38, 42],
    background_color: "#282a36",
    text_color: "#f8f8f2",
    header_background_color: "#282a36",
    modal_background_color: "#44475a",
    modal_backdrop: false,
    preview_label_style: (fg: "#f1fa8c"),
    preview_metadata_group_style: (fg: "#f1fa8c", modifiers: "Bold"),
    tab_bar: (
        active_style: (fg: "#282a36", bg: "#bd93f9", modifiers: "Bold"),
        inactive_style: (fg: "#6272a4"),
    ),
    highlighted_item_style: (fg: "#50fa7b", modifiers: "Bold"),
    current_item_style: (fg: "#282a36", bg: "#50fa7b", modifiers: "Bold"),
    borders_style: (fg: "#6272a4"),
    highlight_border_style: (fg: "#bd93f9"),
    symbols: (
        song: "üéµ",
        dir: "üìÅ",
        playlist: "üìú",
        marker: "‚ñ∂",
        ellipsis: "...",
        song_style: (fg: "#8be9fd"),
        dir_style: (fg: "#ffb86c"),
        playlist_style: (fg: "#ff79c6"),
    ),
    level_styles: (
        info: (fg: "#8be9fd"),
        warn: (fg: "#ffb86c"),
        error: (fg: "#ff5555"),
        debug: (fg: "#50fa7b"),
        trace: (fg: "#ff79c6"),
    ),
    progress_bar: (
        symbols: ["[", "‚îÅ", "‚ñ∂", " ", "]"],
        track_style: (fg: "#44475a"),
        elapsed_style: (fg: "#bd93f9"),
        thumb_style: (fg: "#bd93f9", bg: "#44475a"),
        use_track_when_empty: true,
    ),
    scrollbar: (
        symbols: ["‚îÇ", "‚ñà", "‚ñ≤", "‚ñº"],
        track_style: (),
        ends_style: (),
        thumb_style: (fg: "#bd93f9"),
    ),
    song_table_format: [
        (
            prop: (kind: Property(Artist),
                default: (kind: Text("Unknown"))
            ),
            width: "20%",
        ),
        (
            prop: (kind: Property(Title),
                default: (kind: Text("Unknown"))
            ),
            width: "35%",
        ),
        (
            prop: (kind: Property(Album), style: (fg: "#f8f8f2"),
                default: (kind: Text("Unknown Album"), style: (fg: "#f8f8f2"))
            ),
            width: "30%",
        ),
        (
            prop: (kind: Property(Duration),
                default: (kind: Text("-"))
            ),
            width: "15%",
            alignment: Right,
        ),
    ],
    components: {},
    layout: Split(
        direction: Vertical,
        panes: [
            (
                pane: Pane(Header),
                size: "2",
            ),
            (
                pane: Pane(Tabs),
                size: "3",
            ),
            (
                pane: Pane(TabContent),
                size: "100%",
            ),
            (
                pane: Pane(ProgressBar),
                size: "1",
            ),
        ],
    ),
    header: (
        rows: [
            (
                left: [
                    (kind: Text("["), style: (fg: "#f1fa8c", modifiers: "Bold")),
                    (kind: Property(Status(StateV2(playing_label: "Playing", paused_label: "Paused", stopped_label: "Stopped"))), style: (fg: "#f1fa8c", modifiers: "Bold")),
                    (kind: Text("]"), style: (fg: "#f1fa8c", modifiers: "Bold"))
                ],
                center: [
                    (kind: Property(Song(Title)), style: (modifiers: "Bold"),
                        default: (kind: Text("No Song"), style: (modifiers: "Bold"))
                    )
                ],
                right: [
                    (kind: Property(Widget(ScanStatus)), style: (fg: "#8be9fd")),
                    (kind: Property(Widget(Volume)), style: (fg: "#8be9fd"))
                ]
            ),
            (
                left: [
                    (kind: Property(Status(Elapsed))),
                    (kind: Text(" / ")),
                    (kind: Property(Status(Duration))),
                    (kind: Text(" (")),
                    (kind: Property(Status(Bitrate))),
                    (kind: Text(" kbps)"))
                ],
                center: [
                    (kind: Property(Song(Artist)), style: (fg: "#f1fa8c", modifiers: "Bold"),
                        default: (kind: Text("Unknown"), style: (fg: "#f1fa8c", modifiers: "Bold"))
                    ),
                    (kind: Text(" - ")),
                    (kind: Property(Song(Album)),
                        default: (kind: Text("Unknown Album"))
                    )
                ],
                right: [
                    (
                        kind: Property(Widget(States(
                            active_style: (fg: "#ff79c6", modifiers: "Bold"),
                            separator_style: (fg: "#6272a4")))
                        ),
                        style: (fg: "#6272a4")
                    ),
                ]
            ),
        ],
    ),
    browser_song_format: [
        (
            kind: Group([
                (kind: Property(Track)),
                (kind: Text(" ")),
            ])
        ),
        (
            kind: Group([
                (kind: Property(Artist)),
                (kind: Text(" - ")),
                (kind: Property(Title)),
            ]),
            default: (kind: Property(Filename))
        ),
    ],
    lyrics: (
        timestamp: false
    )
)